body{
				margin:0 auto;
				width: 100%;
				background:#EFF0EA;
				animation: fade 2s ;

			} 
@keyframes fade {
						from {
							opacity: 0;
						}
						to {
							opacity: 1;
						}
					}

.left1 {
				 margin:0 auto;
				 height: auto;
				 width: 36%; 
				 float: left;
				 padding:2% 4% 0% 6%;
			}
.right1 {
				 margin:0 auto;
				 height: auto;
				 width: 46%; 
				 float: left;
 				 padding:2% 6% 0% 2%;
				 overflow: hidden;
			}
.resumebottom{
				 margin:0 auto;
				 height: auto;
				 width: 92%; 
				 clear:left;
				 padding:2% 2% 0% 6%;
				 overflow: hidden;
			}
